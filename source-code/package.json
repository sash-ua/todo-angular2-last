{
  "name": "angular-ToDo",
  "version": "3.1.0",
  "scripts": {
    "start": "gulp",
    "prod": "gulp prod",
    "copy:aot": "gulp copyDist:aot",
    "ngc:aot": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js",
    "map": "source-map-explorer aot/dist/build.js",
    "1.test": "karma start karma.conf.js",
    "2.test": "remap-istanbul -i reports/coverage.json -o reports/coverage -t html && npm run test:launch-webserver",
    "test:launch-webserver": "gulp webserver:test",
    "lite": "lite-server -o",
    "serve": "http-server -p 9091 -c-1 -o",
    "lite:aot": "lite-server -c aot/bs-config.json",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "tsc:aot": "tsc -p tsconfig-aot.json",
    "es5": "tsc --target es5 --allowJs aot/dist/build.js --out aot/dist/bundles.js"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ],
  "dependencies": {
    "@angular/common": "^2.4.4",
    "@angular/compiler": "^2.4.4",
    "@angular/compiler-cli": "^2.4.4",
    "@angular/core": "^2.4.4",
    "@angular/forms": "^2.4.4",
    "@angular/http": "^2.4.4",
    "@angular/material": "2.0.0-beta.1",
    "@angular/platform-browser": "^2.4.4",
    "@angular/platform-browser-dynamic": "^2.4.4",
    "@angular/platform-server": "^2.4.4",
    "@angular/router": "^3.4.4",
    "@angular/upgrade": "^2.4.4",
    "@types/core-js": "^0.9.34",
    "@types/firebase": "^2.4.30",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^7.0.4",
    "angular-in-memory-web-api": "^0.2.4",
    "core-js": "^2.4.1",
    "es6-shim": "^0.35.1",
    "firebase": "^3.6.0",
    "hammerjs": "^2.0.8",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-remap-istanbul": "^0.4.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-beta.12",
    "rxjs-es": "^5.0.0-beta.12",
    "systemjs": "^0.20.1",
    "typings": "^2.0.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "browser-sync": "^2.18.2",
    "concurrently": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean-css": "^2.0.12",
    "gulp-gzip": "^1.4.0",
    "gulp-rimraf": "^0.2.0",
    "gulp-sass": "^3.1.0",
    "gulp-svg-sprite": "^1.3.5",
    "gulp-watch": "^4.3.11",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "lite-server": "^2.2.2",
    "remap-istanbul": "^0.8.4",
    "rollup": "^0.41.4",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "source-map-explorer": "^1.3.3",
    "typescript": "2.2.1",
    "typings": "^2.0.0"
